algoritmo "semnome"
var

senha: caractere
usu: caractere



inicio
usu <-"D"
senha <-"D"
enquanto usu=senha faca

escreva("informe o nome de usúario: ")
leia (usu)
escreva("informe sua senha: ")
leia (senha)

se usu=senha entao
   escreval("Login inválido")
senao
   escreval("Logado!")
fimse


fimenquanto


fimalgoritmo